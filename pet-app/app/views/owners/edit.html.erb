<center>
<h1>Edit Profile</h1>
<hr>
<%=form_for @owner do |f|%>
    <%=f.label "First Name"%>
    <%=f.text_field "first_name"%>
    <%=f.label "Last Name"%>
    <%=f.text_field "last_name"%>
    <%=f.label "Age"%>
    <%=f.number_field "age"%>
    <%=f.label "Email Address"%>
    <%=f.text_field "email"%>
    <%=f.submit "Update Profile"%>
<%end%>
<p>
<hr>
<p><a href="/owners"><button>Cancel and Go Back</button></a>
</center>